# QuizBoutiqueBot Environment Variables
# Copy this file to .env and fill in your values

# Required
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=

# Optional - General Settings
QBB_LANGUAGE=en
QBB_PARSE_MODE=HTML
QBB_LOG_TO_FILE=false
QBB_LOG_LEVEL=INFO

# Optional - Proxy Settings
QBB_PROXY_ENABLED=false
QBB_PROXY_HOST=
QBB_PROXY_PORT=1080
QBB_PROXY_PROTOCOL=socks
QBB_PROXY_USERNAME=
QBB_PROXY_PASSWORD=
